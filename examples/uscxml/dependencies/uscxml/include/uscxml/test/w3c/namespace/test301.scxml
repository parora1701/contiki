<?xml version="1.0" encoding="UTF-8"?>
<!-- the processor should  reject this document because it can't download the script.  
Therefore  we fail if it runs at all. This test is valid only for datamodels that support scripting -->
<scxml:scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance" xmlns:scxml="http://www.w3.org/2005/07/scxml" version="1.0" initial="s0">
  <scxml:script src="D:\foo"/>
  <scxml:state id="s0">
    <scxml:transition target="fail"/>
  </scxml:state>
  <scxml:final id="pass">
    <scxml:onentry>
      <scxml:log label="Outcome" expr="'pass'"/>
    </scxml:onentry>
  </scxml:final>
  <scxml:final id="fail">
    <scxml:onentry>
      <scxml:log label="Outcome" expr="'fail'"/>
    </scxml:onentry>
  </scxml:final>
</scxml:scxml>
