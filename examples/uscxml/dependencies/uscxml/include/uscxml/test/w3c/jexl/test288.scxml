<?xml version="1.0" encoding="UTF-8"?>
<!-- a simple test that a legal value may be assigned to a valid data model location 
using child content -->
<scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance" datamodel="jexl" version="1.0" initial="s0">
  <datamodel>
    <data id="Var1" expr="0"/>
  </datamodel>
  <state id="s0">
    <onentry>
      <assign location="Var1">123</assign>
    </onentry>
    <transition cond="Var1 == 123" target="pass"/>
    <transition target="fail"/>
  </state>
  <final id="pass">
    <onentry>
      <log label="Outcome" expr="'pass'"/>
    </onentry>
  </final>
  <final id="fail">
    <onentry>
      <log label="Outcome" expr="'fail'"/>
    </onentry>
  </final>
</scxml>
