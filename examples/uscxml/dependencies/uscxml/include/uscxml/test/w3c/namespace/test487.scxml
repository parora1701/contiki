<?xml version="1.0" encoding="UTF-8"?>
<!-- test illegal assignment.  error.execution should be raised.  -->
<scxml:scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance" xmlns:scxml="http://www.w3.org/2005/07/scxml" initial="s0" datamodel="ecmascript" version="1.0">
  <scxml:datamodel>
    <scxml:data id="Var1"/>
  </scxml:datamodel>
  <scxml:state id="s0">
    <scxml:onentry>
      <scxml:assign location="Var1" expr="return"/>
      <scxml:raise event="event"/>
    </scxml:onentry>
    <scxml:transition event="error.execution" target="pass"/>
    <scxml:transition event="*" target="fail"/>
  </scxml:state>
  <scxml:final id="pass">
    <scxml:onentry>
      <scxml:log label="Outcome" expr="'pass'"/>
    </scxml:onentry>
  </scxml:final>
  <scxml:final id="fail">
    <scxml:onentry>
      <scxml:log label="Outcome" expr="'fail'"/>
    </scxml:onentry>
  </scxml:final>
</scxml:scxml>
